{
  "Application": {
    "RunAsService": true,
    "PrintTxsInfoEveryXMinutes": 1
  },
  "IotaNode": {
    "ApiUrl": "https://explorer-api.iota.org",
    "MessagesEndPointPath": "/search/mainnet/"
  },
  "Mqtt": {
    "Host": "192.168.11.92",
    "Port": 1883,
    "SslProtocol": "None",
    "UseTls": false,
    "ClientId": "",
    "Username": "",
    "Password": "",
    "ProtocolVersion": "V311",
    "Topics": [ "messages/referenced" ],
    "CleanSession": true
  },
  "Logging": {
    "TxLog": {
      "Path": "Logs/txlog.txt"
    },
    "MinimumLevel": "Verbose",
    "File": {
      "Enabled": true,
      "Path": "Logs/log.txt",
      "RollingInterval": "Day",
      "RollOnFileSizeLimit": true,
      "RestrictedToMinimumLevel": "Verbose"
    },
    "Console": {
      "Enabled": true,
      "RestrictedToMinimumLevel": "Information"
    }
  }
}